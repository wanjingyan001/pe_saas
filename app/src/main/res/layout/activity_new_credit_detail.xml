                <TextView
                    android:id="@+id/stabilityTv8Num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="20dp"
                    android:background="@drawable/bg_stability_type1"
                    android:text="4k～5k"
