                    android:textSize="@dimen/text_size_13"
                    app:layout_constraintBottom_toBottomOf="@+id/stabilityTv8"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/stabilityTv8" />


            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/white">

                <ImageView
                    android:layout_width="4dp"
                    android:layout_height="14dp"
                    android:background="@color/colorPrimary"
                    app:layout_constraintBottom_toBottomOf="@+id/otherInfoTv"
                    app:layout_constraintTop_toTopOf="@+id/otherInfoTv" />

                <TextView
                    android:id="@+id/otherInfoTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:paddingBottom="13dp"
                    android:paddingTop="13dp"
                    android:text="其他信息"
                    android:textColor="@color/text_1"
                    android:textSize="@dimen/text_size_medium"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/tv1"
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:layout_marginStart="20dp"
                    android:gravity="center_vertical"
                    android:text="常工作省份"
                    android:textColor="@color/text_8"
